@model Pojisteni.Models.InsuranceName

@{
    ViewData["Title"] = "Vytvořit pojištění";
}

<h1>Vytvořit pojištění</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="nazevPojisteni" class="control-label"></label>
                <input asp-for="nazevPojisteni" class="form-control" />
                <span asp-validation-for="nazevPojisteni" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Vytvořit pojištění" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
